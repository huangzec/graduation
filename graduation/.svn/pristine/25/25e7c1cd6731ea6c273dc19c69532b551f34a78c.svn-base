HHJsLib.register({
	init: function() {
		this.bindBtnPassClick();
		this.bindBtnRepassClick();
	},
	bindBtnPassClick: function() {
		$("#judge-form").find(".pass").click(function() {
			HHJsLib.info("审核通过");
		});
	},
	bindBtnRepassClick: function() {
		$("#judge-form").find(".repass").click(function() {
			HHJsLib.confirm("确定审核不通过？");
		});
	}
});